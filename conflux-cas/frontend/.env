# Next.js public env vars — safe to commit (no secrets)
# These mirror the NEXT_PUBLIC_* entries in the monorepo-root .env so that
# Next.js can find them without any custom config loading.

NEXT_PUBLIC_AUTOMATION_MANAGER_ADDRESS=0x9D5B131e5bA37A238cd1C485E2D9d7c2A68E1d0F
NEXT_PUBLIC_API_URL=http://localhost:3001
NEXT_PUBLIC_NETWORK=mainnet
# Obtain a project ID from https://cloud.walletconnect.com and set it here
# (or in frontend/.env.local which is gitignored):
NEXT_PUBLIC_WC_PROJECT_ID=replace_with_walletconnect_project_id

# Admin address whitelist — comma-separated, case-insensitive.
# These addresses can see the Safety panel and call pause/resume.
NEXT_PUBLIC_ADMIN_ADDRESSES=0xbc621b293c3a35078d3520dec246e70de40bba15
